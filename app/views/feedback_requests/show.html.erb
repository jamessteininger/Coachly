<%- model_class = FeedbackRequest -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @feedback_request.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sender_id) %>:</strong></dt>
  <dd><%= @feedback_request.sender_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:prompt) %>:</strong></dt>
  <dd><%= @feedback_request.prompt %></dd>
  <dt><strong><%= model_class.human_attribute_name(:b_read) %>:</strong></dt>
  <dd><%= @feedback_request.b_read %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              feedback_requests_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_feedback_request_path(@feedback_request), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              feedback_request_path(@feedback_request),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
